{
  "name": "music-share",
  "version": "1.0.0",
  "description": "share music to anyone on any platform",
  "main": "server.js",
  "author": "juvenile-orca",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/juvenile-orca/songlink"
  },
  "engines": {
    "node": "5.0.0"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --config webpack.production.config.js",
    "postbuild": "npm run static",
    "static": "mkdir dist/ && cp -a server/static/images dist/images/ && node-sass server/templates/linkTemplate/scssStyles/main.scss server/static/styles/main.css && grunt mini",
    "postinstall": "npm run build",
    "start": "node server/server.js",
    "serve": "npm run static && nodemon --debug server/server.js --ignore components --ignore containers",
    "preserve": "rm -f dist/bundle.js && rm -f dist/app.css",
    "preserve:prod": "npm run build",
    "serve:prod": "NODE_ENV=production nodemon --debug server/server.js --ignore components --ignore containers",
    "test": "NODE_ENV=production mocha test/models test/controllers test/server"
  },
  "dependencies": {
    "babel-core": "^5.8.3",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-plugin-react-transform": "^1.1.0",
    "babel-runtime": "^5.8.20",
    "body-parser": "^1.14.1",
    "chai": "^3.4.1",
    "classnames": "^2.2.1",
    "cookie-parser": "^1.4.0",
    "css-loader": "^0.23.0",
    "dotenv": "^1.2.0",
    "express": "^4.13.3",
    "grunt": "^0.4.5",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-cssmin": "^0.14.0",
    "grunt-contrib-uglify": "^0.11.0",
    "extract-text-webpack-plugin": "^0.9.1",
    "history": "^1.13.1",
    "ituner": "^0.1.4",
    "jquery": "^2.1.4",
    "mocha": "^2.3.4",
    "mongo": "^0.1.0",
    "mongoose": "^4.2.9",
    "mustache": "^2.2.0",
    "node-sass": "^3.4.2",
    "react": "^0.14.3",
    "react-clipboard.js": "^0.1.5",
    "react-dom": "^0.14.3",
    "react-radio-group": "^2.2.0",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0",
    "react-social": "^1.0.2",
    "react-tooltip": "^0.9.5",
    "react-transform-hmr": "^1.0.1",
    "redux": "^3.0.4",
    "redux-logger": "^2.0.4",
    "redux-thunk": "^1.0.0",
    "request": "^2.67.0",
    "sass-loader": "^3.1.2",
    "spotify": "^0.3.0",
    "style-loader": "^0.13.0",
    "supertest": "^1.1.0",
    "underscore": "^1.8.3",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.5.0",
    "youtube-node": "^1.3.0",
    "youtube-node-with-params": "^1.0.2",
    "youtube-search": "^1.0.4"
  },
  "devDependencies": {
    "node-sass": "^3.4.2"
  }
}
